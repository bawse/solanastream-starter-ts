# NATS (Core client)
# Prefer full server URL; falls back to host/port if unset
NATS_SERVER_URL=nats://localhost:4222
NATS_HOST=localhost
NATS_PORT=4222
NATS_NAME=solana-stream-consumer

# Auth (choose ONE)
# 1) Credentials file (.creds containing both JWT and NKEY seed)
NATS_JWT_FILE=./jwt.creds
# or NATS_CREDS_FILE=./jwt.creds

# 2) Inline pair (avoid committing secrets)
# NATS_USER_JWT=
# NATS_USER_SEED=

# Streams
STREAM_NAMES=basic-stream
STREAM_SUBJECTS=basic.pumpfun

# Logging & Stats
LOG_LEVEL=info
STATS_INTERVAL=5000

